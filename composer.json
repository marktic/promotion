{
  "name": "marktic/promotion",
  "type": "library",
  "license": "MIT",
  "description": "promotion base objects",
  "keywords": [],
  "homepage": "https://github.com/marktic/promotion",
  "authors": [
    {
      "name": "Gabriel Solomon",
      "email": "hello@gabrielsolomon.ro"
    }
  ],
  "autoload": {
    "psr-4": {
      "Marktic\\Promotion\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Marktic\\Promotion\\Tests\\": "tests/src",
      "Marktic\\Promotion\\Tests\\Fixtures\\": "tests/fixtures",
      "Marktic\\Promotion\\Tests\\Fixtures\\Application\\Models\\": "tests/fixtures/application/models"
    }
  },
  "require": {
    "ext-json": "*",
    "bytic/package-base": "^1.0",
    "bytic/admin-base": "^1.0",
    "bytic/console": "^1.0",
    "bytic/controllers": "^1.0",
    "bytic/controllers-extra": "^1.0",
    "bytic/event-dispatcher": "^1.0",
    "bytic/form": "^1.0",
    "bytic/orm": "^0.9|^1.0",
    "bytic/orm-filters": "^0.9|^1.0",
    "bytic/orm-behaviors": "^0.9|^1.0",
    "bytic/translation": "^0.9|^1.0",
    "bytic/models-smart-properties": "^1.0",
    "bytic/migrations": "^0.12",
    "marktic/pricing": "dev-main",
    "bytic/assert": "^0.1"
  },
  "require-dev": {
    "bytic/phpqatools": "^1.0"
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "config": {
    "allow-plugins": {
      "phpstan/extension-installer": true,
      "ergebnis/composer-normalize": true,
      "dealerdirect/phpcodesniffer-composer-installer": true,
      "bytic/phpqatools": true
    }
  }
}
